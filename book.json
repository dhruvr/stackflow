{
    "plugins": ["github-issue-feedback", "ghcomment", "github-edit", "disqus", "page-toc", "summary", "expandable-chapters", "-lunr", "-search", "search-plus", "puml"],
    "pluginsConfig": {
        "disqus": {
            "shortName": "stackflow"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "expandable-chapters":{},
        "github-edit": {
            "repo": "stackflow-io/stackflow",
            "branch": "master"
        },
        "github-issue-feedback": {
            "repo": "stackflow-io/stackflow"
        },
        "ghcomment": {
            "owner": "stackflow-io",
            "repo": "stackflow"
          }
    }
}
