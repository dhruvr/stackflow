{
    "plugins": ["disqus", "page-toc", "summary", "expandable-chapters", "-lunr", "-search", "search-plus", "puml"],
    "pluginsConfig": {
        "disqus": {
            "shortName": "stackflow"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "expandable-chapters":{}
    }
}
